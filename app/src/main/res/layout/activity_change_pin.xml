<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="idv.swj.dentist.ChangePinActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView32"
        android:layout_width="75dp"
        android:layout_height="83dp"
        android:text="@string/passwordRequest"
        android:textColor="#40000000"
        android:textSize="12sp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/fpAccount"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/fpAccount" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="90dp"
        android:layout_height="18dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/rePassword"
        android:textSize="16sp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/textView34"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="@+id/fpRePassword"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/oldPassword"
        android:textSize="16sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/fpPassword"
        app:layout_constraintTop_toTopOf="@+id/fpOldPassword" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/newPassword"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/fpPassword"
        app:layout_constraintTop_toTopOf="@+id/fpPassword" />

    <EditText
        android:id="@+id/fpPassword"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/fpAccount"
        app:layout_constraintRight_toLeftOf="@+id/textView32"
        app:layout_constraintTop_toBottomOf="@+id/fpOldPassword" />

    <EditText
        android:id="@+id/fpRePassword"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintLeft_toLeftOf="@+id/fpPassword"
        app:layout_constraintRight_toRightOf="@+id/fpPassword"
        app:layout_constraintTop_toBottomOf="@+id/fpPassword" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="90dp"
        android:layout_height="22dp"
        android:text="@string/acconut"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@+id/fpAccount"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/fpAccount"
        android:layout_width="239dp"
        android:layout_height="47dp"
        android:text="TextView"
        android:textSize="16sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView26"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:onClick="onClickSubmit"
        android:text="@string/submit"
        app:layout_constraintRight_toRightOf="@+id/textView32"
        app:layout_constraintTop_toBottomOf="@+id/fpRePassword" />

    <EditText
        android:id="@+id/fpOldPassword"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        android:layout_marginTop="0dp"
        app:layout_constraintRight_toLeftOf="@+id/textView32"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/fpAccount"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/textView32" />
</android.support.constraint.ConstraintLayout>
